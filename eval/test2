function f(var x)
    {
    lambda (var y) { x + y; };
    }

function main(){
var a = f(3);
var b = f(11);

println("a(5) is ",a(5));      ~~# should be 8
println("b(6) is ",b(6));      ~~# should be 17
println("a(10) is ",a(10));    ~~# should be 13

~~This also serves as a lambda test. 
}